{{ form_start(form) }}

    <div class="row">
        <div class="col">
            {{ form_row(form.title) }}

            <strong>{{ form_label(form.created) }}</strong>
            {{ form_widget(form.created) }}
        </div>
        <div class="col">
            {{ form_row(form.content) }}
        </div>
        
    </div>
<button class="btn">{{ button_label|default('Save') }}</button>

    
{{ form_end(form) }}